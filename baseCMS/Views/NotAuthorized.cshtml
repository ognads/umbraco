@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.NotAuthorized>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
    Layout = "Master.cshtml";
}
<section class="content" style="height:100%">

    <div class="error-page">
        <h2 class="headline text-red">401</h2>

        <div class="error-content">
            <h3><i class="fa fa-warning text-red"></i> Oops! You do not have the required authorization.</h3>

            <p>
                If you think this is an error please contact your administrator.
                Meanwhile, you may <a href="/">return to dashboard</a> or try using the search form.
            </p>
        </div>
    </div>
    <!-- /.error-page -->

</section>
